{
  "name": "@sublet/hulk-express-server",
  "version": "0.13.3",
  "description": "Pre-built Express server that allows you to get up and running quickly.",
  "main": "src/index.js",
  "scripts": {
    "prebuild:prod": "rm -rf dist && mkdir dist",
    "build:prod": "cp -r src/* dist",
    "postbuild:prod": "make publish-post",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "test": "nyc --reporter=html --reporter=text mocha test/**/*.* --exit",
    "test:unit": "mocha test/unit/*.* --exit",
    "test:all": "mocha test/**/*.* --exit"
  },
  "files": [
    "_templates/**/*",
    "src/**/*",
    "bin/**/*"
  ],
  "bin": {
    "hulk": "bin/cli.js"
  },
  "engines": {
    "node": ">=12.17.0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "chalk": "4.1.2",
    "commander": "5.1.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "enquirer": "2.3.6",
    "express": "4.17.1",
    "express-session": "1.17.2",
    "hygen": "6.1.0",
    "jsonwebtoken": "8.5.1",
    "ldapjs": "2.3.1",
    "lodash": "4.17.21",
    "npm-run-path": "4.0.1",
    "response-time": "2.3.2",
    "uuidv4": "6.2.12",
    "winston": "3.3.3",
    "winston-3-papertrail": "1.0.7"
  }
}
